{
  "manifest_version": 3,
  "name": "My Awesome Extension",
  "version": "1.0",
  "description": "Description of your extension",
  "background": {
    "service_worker": "src/index.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/index.js"]
    }
  ],
  "permissions": ["activeTab"]
}
